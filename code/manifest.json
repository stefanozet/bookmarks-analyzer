{
	"manifest_version": 3,
	"name": "Bookmarks Analizer",
	"version": "1.0",
	"description": "Bookmarks analyzer help to delete duplicate bookmarks and empty folders.",
	"permissions": [
		"bookmarks"
	],
	"action": {
		"default_icon": "icons/icon128.png"
	},
	"web_accessible_resources": [
		{
			"resources": [
				"bookmarksManager.html"
			],
			"matches": [
				"<all_urls>"
			]
		}
	],
	"background": {
  	"service_worker": "background.js"
	}
}